<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/firstFragment2">

    <fragment
        android:id="@+id/firstFragment2"
        android:name="com.geektech.hw5.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment2_to_secondFragment2"
            app:destination="@id/secondFragment2" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment2"
        android:name="com.geektech.hw5.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >

        <action
            android:id="@+id/action_secondFragment2_to_thirdFragment2"
            app:destination="@id/thirdFragment2" />
    </fragment>
    <fragment
        android:id="@+id/fifthFragment2"
        android:name="com.geektech.hw5.FifthFragment"
        android:label="fragment_fifth"
        tools:layout="@layout/fragment_fifth" >

        <action
            android:id="@+id/action_fifthFragment2_to_firstFragment2"
            app:destination="@id/firstFragment2" />
    </fragment>
    <fragment
        android:id="@+id/fourthFragment2"
        android:name="com.geektech.hw5.FourthFragment"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_fourth" >
        <action
            android:id="@+id/action_fourthFragment2_to_fifthFragment2"
            app:destination="@id/fifthFragment2" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment2"
        android:name="com.geektech.hw5.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" >
        <action
            android:id="@+id/action_thirdFragment2_to_fourthFragment2"
            app:destination="@id/fourthFragment2" />
    </fragment>
</navigation>